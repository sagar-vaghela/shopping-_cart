{
    "env": {
      "browser": true,
      "jest": true,
      "es2021": true
    },
   "rules": {
      "react/react-in-jsx-scope": "off"
   },
    "extends": [
      "eslint:recommended",
      "plugin:react/recommended",
      "plugin:@next/next/recommended"
    ],
    "parserOptions": {
      "ecmaFeatures": {
        "jsx": true
      },
      "ecmaVersion": 12,
      "sourceType": "module"
    },
    "plugins": [
      "react",
      "@next/next"
    ],
    "settings": {
      "react": {
        "version": "detect"
      }
    }
  
   
}

  